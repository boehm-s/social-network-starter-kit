{
			"name": "users-microservice",
			"keywords": [],
			"author": "Steven BOEHM",
			"license": "ISC",
			"version": "0.0.1",
			"description": "Simple app with users interactions",
			"repository": {
						"type": "git",
						"url": "git+https://github.com/boehm-s/users-microservice"
			},
			"bugs": {
						"url": "https://github.com/boehm-s/users-microservice/issues"
			},
			"homepage": "https://github.com/boehm-s/users-microservice#readme",
			"babel": {
						"presets": [
									"es2015",
									"stage-2"
						],
						"plugins": [
									[
												"transform-runtime",
												{
															"polyfill": false,
															"regenerator": true
												}
									]
						]
			},
			"main": "app.js",
			"scripts": {
						"start": "node app.js",
						"dev": "node_modules/.bin/nodemon  --inspect=0.0.0.0:9229 node_modules/.bin/babel-node src/app.js",
						"test": "mocha -r babel-polyfill --compilers js:babel-register --recursive --reporter spec --timeout 25000",
						"coverage": "BABEL_ENV=test nyc --require babel-polyfill --require babel-register mocha --recursive --timeout 25000"
			},
			"dependencies": {
						"body-parser": "^1.18.3",
						"bookshelf": "^0.13.3",
						"cookie-parser": "^1.4.3",
						"express": "*",
						"express-session": "^1.15.6",
						"morgan": "^1.9.0",
						"pg": "^7.4.3",
						"should": "^13.2.3"
			},
			"devDependencies": {
						"babel-cli": "*",
						"babel-plugin-transform-runtime": "^6.23.0",
						"babel-preset-es2015": "^6.14.0",
						"babel-preset-stage-2": "*",
						"babel-runtime": "^6.26.0",
						"chai": "^4.1.2",
						"eslint": "*",
						"eslint-plugin-async-await": "*",
						"eslint-plugin-no-loops": "*",
						"eslint-plugin-unicorn": "*",
						"istanbul": "^0.4.5",
						"knex": "*",
						"mocha": "^5.2.0",
						"nodemon": "*",
						"supertest": "^3.1.0"
			}
}
